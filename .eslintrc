{
  "rules": {
    "indent": 0,
    "quotes": [
      2,
      "double"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-undef": 0,
    "no-console": 0,
    "no-unused-vars": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "meteor": true,
    "mongo": true,
    "commonjs": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  }
}
